{
    "dataRoles": [
        {
            "displayName": "y-Axis Category",
            "name": "category",
            "kind": "Grouping"
        },
        {
            "displayName": "Bar Labels",
            "name": "label",
            "kind": "Grouping"
        },
        {
            "displayName": "Start Date",
            "name": "startDate",
            "kind": "GroupingOrMeasure"
        },
        {
            "displayName": "End Date",
            "name": "endDate",
            "kind": "GroupingOrMeasure"
        }
    ],
    "objects": {
        "colorSelector": {
            "displayName": "Data Colors",
            "properties": {
                "fill": {
                    "displayName": "Color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "yAxis": {
            "displayName": "y-Axis",
            "properties": {
                "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "width": {
                    "displayName": "Label Width",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "xAxis": {
            "displayName": "x-Axis",
            "properties": {
                "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "showMonthDay":{
                    "displayName": "Show Day of Month",
                    "type": {
                        "bool": true
                    }
                },
                "showYear":{
                    "displayName": "Show Year",
                    "type": {
                        "bool": true
                    }
                },
                "datetimeFormat":{
                    "displayName": "Date/Time Format",
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "label": {
            "displayName": "Label",
            "properties": {
                "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    }
                },
                {
                    "label": {
                        "max": 1
                    }
                },
                {
                    "startDate": {
                        "max": 1
                    }
                },
                {
                    "endDate": {
                        "max": 1
                    }
                }
            ],
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "category"
                            }
                        },
                        {
                            "for": {
                                "in": "label"
                            }
                        },
                        {
                            "for": {
                                "in": "startDate"
                            }
                        },
                        {
                            "for": {
                                "in": "endDate"
                            }
                        }
                    ]
                }
            },
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "label"
                            }
                        }
                    ]
                },
                "values": {
                    "select": [
                        {"for": {"in": "category"}},
                        {"for": {"in": "startDate"}},
                        {"for": {"in": "endDate"}}
                    ]
                }
            }
        }
    ],
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": false
        },
        "roles": [
            "Tooltips"
        ]
    }
}